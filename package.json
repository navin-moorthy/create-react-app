{
  "name": "create-react-app",
  "version": "1.0.1",
  "description": "Personal Create React App",
  "keywords": [
    "react",
    "template",
    "create-react-app",
    "typescript",
    "new-project",
    "new-repo"
  ],
  "bugs": {
    "url": "https://github.com/navin-moorthy/create-react-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/navin-moorthy/create-react-app.git"
  },
  "license": "MIT",
  "author": "Navin Moorthy <navin007.a@gmail.com>",
  "scripts": {
    "postinstall": "husky install",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint": "eslint --color --ext .js,.jsx,.ts,.tsx .",
    "lint:fix": "eslint --color --ext .js,.jsx,.ts,.tsx . --fix",
    "format": "prettier -wl \"./**/*.{html,css,js,jsx,ts,tsx,md,json}\"",
    "format:package": "sort-package-json package.json",
    "release": "release-it",
    "commit": "gacp",
    "contrib:init": "all-contributors init",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx,tx,tsx}": [
      "yarn lint:fix"
    ],
    "**/*.{html,css,js,jsx,ts,tsx,md,json}": [
      "yarn format"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "16.0.1",
    "@commitlint/config-conventional": "16.0.0",
    "@release-it/conventional-changelog": "3.3.0",
    "@testing-library/dom": "8.11.1",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/user-event": "13.5.0",
    "@types/jest": "27.0.3",
    "@types/node": "17.0.5",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "all-contributors-cli": "6.20.0",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "gacp": "2.10.2",
    "husky": "7.0.4",
    "lint-staged": "12.1.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "5.0.0",
    "release-it": "14.11.8",
    "sort-package-json": "1.53.1",
    "typescript": "4.5.4",
    "web-vitals": "2.1.2"
  },
  "gacp": {
    "add": false,
    "push": false,
    "emoji": "emoji",
    "editor": false
  }
}
